{
  "name": "a11y-root-extension",
  "displayName": "A11y-Root-Extension",
  "description": "vscode extension ",
  "version": "0.0.1",
  "engines": {
    "vscode": "^1.95.0"
  },
  "categories": [
    "Other"
  ],
  "activationEvents": [],
  "main": "./dist/extension.js",
  "contributes": {
    "commands": [
      {
        "command": "a11y-root-extension.openTab",
        "title": "A11y Root"
      }
    ]
  },
  "scripts": {
    "vscode:prepublish": "npm run package",
    "compile": "webpack",
    "watch": "concurrently \"webpack --watch --stats-error-details\" \"npx tsc --project tsconfig.json --watch\" \"npx tsc --project tsconfig.webview.json --watch\"",
    "package": "webpack --mode production --devtool hidden-source-map",
    "compile-tests": "tsc -p . --outDir out",
    "watch-tests": "tsc -p . -w --outDir out",
    "pretest": "npm run compile-tests && npm run compile && npm run lint",
    "lint": "eslint src",
    "test": "vscode-test",
    "build:webview": "webpack --config webpack.webview.config.js",
    "serve:webview": "webpack serve --config webpack.webview.config.js",
    "build:extension": "webpack --config webpack.config.js",
    "build": "npx tsc --project tsconfig.json && npx tsc --project tsconfig.webview.json"
  },
  "devDependencies": {
    "@types/mocha": "^10.0.9",
    "@types/node": "20.x",
    "@types/vscode": "^1.95.0",
    "@typescript-eslint/eslint-plugin": "^8.10.0",
    "@typescript-eslint/parser": "^8.7.0",
    "@vscode/test-cli": "^0.0.10",
    "@vscode/test-electron": "^2.4.1",
    "bufferutil": "^4.0.8",
    "concurrently": "^9.1.0",
    "css-loader": "^7.1.2",
    "eslint": "^9.13.0",
    "html-webpack-plugin": "^5.6.3",
    "style-loader": "^4.0.0",
    "ts-loader": "^9.5.1",
    "typescript": "^5.6.3",
    "utf-8-validate": "^6.0.5",
    "webpack": "^5.97.0",
    "webpack-cli": "^5.1.4",
    "webpack-dev-server": "^5.1.0"
  },
  "dependencies": {
    "puppeteer": "^23.9.0",
    "puppeteer-core": "^23.10.0"
  }
}
